{
  "id": "acre-{{id}}",
  "name": "{{name}}",
  "url": "{{{url}}}",
  "homepageUrl": "https:/acre.fi/",
  "icon": "{{{url}}}acre.svg",
  "platforms": "desktop",
  "apiVersion": "^2.0.0",
  "manifestVersion": "2",
  "branch": "stable",
  "categories": ["staking"],
  "currencies": ["bitcoin", "bitcoin_testnet"],
  "content": {
    "shortDescription": {
      "en": "Bitcoin Liquid Staking"
    },
    "description": {
      "en": "Bitcoin Liquid Staking"
    }
  },
  "permissions": [
    "account.request",
    "account.list",
    "message.sign",
    "transaction.sign",
    "transaction.signAndBroadcast",
    "bitcoin.getXPub"
  ],
  "domains": ["http://*"],
  "type": "walletApp"
}
