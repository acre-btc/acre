{
  "id": "{{id}}",
  "name": "{{name}}",
  "url": "{{{url}}}?embed=ledger-live",
  "homepageUrl": "https:/acre.fi/",
  "icon": "{{{url}}}acre.svg",
  "platforms": "desktop",
  "apiVersion": "^2.0.0",
  "manifestVersion": "2",
  "branch": "stable",
  "categories": ["staking"],
  "currencies": ["bitcoin", "bitcoin_testnet"],
  "content": {
    "shortDescription": {
      "en": "Bitcoin Liquid Staking"
    },
    "description": {
      "en": "Bitcoin Liquid Staking"
    }
  },
  "permissions": [
    "account.request",
    "account.list",
    "bitcoin.getAddress",
    "bitcoin.getPublicKey",
    "custom.acre.messageSign",
    "custom.acre.transactionSignAndBroadcast"
  ],
  "domains": ["http://*"],
  "type": "walletApp"
}
